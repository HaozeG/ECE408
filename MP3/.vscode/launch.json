{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "CUDA C++: Attach",
            "type": "cuda-gdb",
            "request": "attach",
            "processId": "${command:cuda.pickProcess}"
        },
        {
            "name": "CUDA C++: Launch",
            "type": "cuda-gdb",
            "request": "launch",
            "program": "${workspaceFolder}/MP3/template",
            "args": "${workspaceFolder}/MP3/data/1/output.raw -i ${workspaceFolder}/MP3/data/1/input0.raw ${workspaceFolder}/MP3/data/1/input1.raw -t vector"
        }
    ]
}